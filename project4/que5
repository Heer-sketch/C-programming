#include <stdio.h>

int main() {
    int i, j;

    for(i = 5; i >= 2; i--) {

        for(j = i; j <= 5; j++) {
            printf("%d ", j);
        }

        for(j = 5-1; j >= i; j--) {
            printf("%d ", j);
        }

        printf("\n");
    }

    for(i = 1; i <= 5; i++) {
        printf("%d ", i);
    }
    for(i = 4; i >= 1; i--) {
        printf("%d ", i);
    }
    printf("\n");

    return 0;
}
