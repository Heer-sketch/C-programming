#include <stdio.h>

int main() {
    int i, j;

    for(i = 1; i <= 5; i++) {

        if(i == 1 || i == 3) {

            for(j = 1; j <= 5; j++) {
                printf("* ");
            }
        } else if(i == 2) {

            for(j = 1; j <= 2; j++) {
                printf("* ");
            }
        } else {

            printf("* ");
        }
        printf("\n");
    }

    return 0;
}
